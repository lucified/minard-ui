{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": [false, "always-prefix"],
    "quotemark": [true, "single", "avoid-escape", "jsx-double"],
    "no-var-requires": false,
    "jsx-alignment": true,
    "jsx-no-lambda": true,
    "jsx-no-string-ref": true,
    "jsx-self-close": true
  },
  "rulesDirectory": []
}
