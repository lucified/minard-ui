  "version": "0.0.1",
  "description": "Minard frontend",
  "main": "index.js",
  "scripts": {
    "start": "npm run-script webpack-dev-server",
    "clean": "rm -rf dist",
    "build": "npm run-script clean && webpack",
    "test": "npm run compile && mocha lib/test",
    "deploy": "npm run-script github-deploy-notify && npm run-script build && npm run-script circle-artifact && npm run-script atomic-s3",
    "atomic-s3": "atomic-s3 --verbose",
    "github-deploy-notify": "github-deploy-notify --verbose",
    "flowdock-deploy-notify": "flowdock-deploy-notify --verbose",
    "webpack-dev-server": "webpack-dev-server --config=webpack.config.dev.js --inline --hot --port 3000 --host 0.0.0.0",
    "compile": "tsc",
    "circle-artifact": "node circle-artifact.js"
  },
  "engines": {
    "node": "^6.0.0"
  },
  "homepage": "https://gitub.com/lucified/minard-ui",
