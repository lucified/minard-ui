{
  "name": "minard-ui",
  "license": "UNLICENSED",
  "version": "0.0.1",
  "description": "Minard frontend",
  "main": "index.js",
  "scripts": {
    "start": "./start.sh",
    "clean": "rm -rf dist",
    "build": "npm run clean && webpack",
    "compiled-test": "npm run clean-compile && npm run compile && cp -R json lib/ && mocha lib/test",
    "test": "mocha --opts mocha.opts",
    "test:watch": "mocha --opts mocha.auto.opts --watch",
    "deploy": "lucify-notifier npm run deploy-plain",
    "deploy-plain": "npm run build && npm run atomic-s3",
    "deploy-chatops": "[ \"$AWS_TEMPORARY\" ] && npm run deploy || echo 'Skipping deployment'",
    "atomic-s3": "atomic-s3 --verbose",
    "webpack-dev-server": "webpack-dev-server --config=webpack.config.dev.js --hot --port 3000 --host 0.0.0.0",
    "clean-compile": "rm -rf lib",
    "compile": "tsc",
    "lint": "tslint --format=verbose 'src/js/**/*.ts' 'src/js/**/*.tsx'",
    "watch": "webpack --progress --colors --watch"
  },
  "engines": {
    "node": "^6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "homepage": "https://gitub.com/lucified/minard-ui",
