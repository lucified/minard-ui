{
  "name": "minard-ui",
  "license": "UNLICENSED",
  "version": "0.0.1",
  "description": "Minard frontend",
  "main": "index.js",
  "scripts": {
    "start": "npm run webpack-dev-server",
    "clean": "rm -rf dist",
    "build": "npm run clean && webpack",
    "compiled-test": "npm run clean-compile && npm run compile && cp -R json lib/ && mocha --require mock-local-storage --reporter mocha-junit-reporter lib/**/*.spec.js",
    "test": "mocha --opts mocha.opts",
    "test:watch": "mocha --opts mocha.auto.opts --watch",
    "deploy": "lucify-notifier npm run deploy-plain",
    "deploy-plain": "npm run build && npm run atomic-s3",
    "deploy-chatops": "[ -z \"$AWS_TEMPORARY\" ] || yarn run deploy",
    "atomic-s3": "atomic-s3 --verbose",
    "webpack-dev-server": "webpack-dev-server --config=webpack.config.dev.js --hot --port 3000 --host 0.0.0.0",
    "clean-compile": "rm -rf lib",
    "compile": "tsc",
    "lint": "tslint -t verbose 'src/js/**/*.ts' 'src/js/**/*.tsx'",
    "watch": "webpack --progress --colors --watch",
    "prettify": "prettier --parser typescript --single-quote --trailing-comma all --write \"src/**/*.ts?(x)\" \"src/**/*.scss\"",
    "lint-staged-stash": "git stash save --keep-index 'lint-staged' && touch .didstash || rm .didstash || true",
    "lint-staged-stash-pop": "test -f .didstash && rm .didstash && git stash pop || true",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": "^7.0.0"
  },
  "lint-staged": {
    "*.js?(x)": [
      "lint-staged-stash",
      "prettier --single-quote --trailing-comma all --write",
      "git add",
      "lint-staged-stash-pop"
    ],
    "*.ts?(x)": [
      "lint-staged-stash",
      "prettier --parser typescript --single-quote --trailing-comma all --write",
      "git add",
      "lint-staged-stash-pop"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "homepage": "https://gitub.com/lucified/minard-ui",
  "contributors": [
    {
      "name": "Juho Ojala",
      "email": "juho@lucify.com"
    },
    {
      "name": "Ville Saarinen",
      "email": "ville.saarinen@lucify.com"
    },
    {
      "name": "Ville Väänänen",
      "email": "ville@lucify.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucified/minard-ui.git"
  },
