    "file-loader": "^0.9.0",
    "flowdock-deploy-notify": "^0.1.5",
    "github-deploy-notify": "~0.1.8",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.1.4",
    "html-webpack-plugin": "^2.22.0",
    "lucify-deploy-config": "^0.1.8",
    "mocha": "^2.5.3",
